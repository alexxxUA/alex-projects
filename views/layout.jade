doctype
html
	head
		title Explorer
		include metaInclude
		include stylesInclude
		block pageConfigValues
		include scriptInclude
	body(class= user && user.isLogged == true ? 'user-logged' : '')
		#section-container
			#header
				h1
					a(href="/", title="Go to home page")
						span Expl
						span.logo
							include ./../static/img/icon.svg
						span rer
					-if(title)
						span - #{title}
				block facebookLogin
			#content(class='clearfix')
				#dynamicContent
					block explorer
				block error404
				block adminPanel
		include uploadSection
		block contextMenu
		#loading
			img(src="/img/ajax-loader1.gif", width="32", height="32", alt="loader")
		.js-ajax-msg